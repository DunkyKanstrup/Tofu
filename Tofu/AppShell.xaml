    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="newPig"
        ContentTemplate="{DataTemplate local:pigPage}"
        Route="pigPage" />
</Shell>
